# OpenClaw Clawdbot Production Profile
# Production configuration with explicit feature enablement
# All external interactions are enabled and monitored

# Environment mode
CLAWDBOT_ENV=production

# Safety flags (production enabled, but with proper oversight)
CLAWDBOT_SAFE_MODE=false
CLAWDBOT_ALLOW_EXTERNAL_SEND=true
CLAWDBOT_ALLOW_EXTERNAL_CALLS=true
CLAWDBOT_ALLOW_PAYMENTS=true

# Secrets and credentials (use prod scope via secrets manager)
CLAWDBOT_SECRETS_SCOPE=prod

# Service behaviors
CLAWDBOT_N8N_DRY_RUN=false
CLAWDBOT_LOG_LEVEL=info

# Production behaviors
CLAWDBOT_ENABLE_TELEMETRY=true
CLAWDBOT_ENABLE_CRASH_REPORTING=true
CLAWDBOT_MAX_WORKERS=8

# Gateway settings for production
CLAWDBOT_GATEWAY_MODE=managed
CLAWDBOT_GATEWAY_PORT=443

# Production API endpoints
CLAWDBOT_API_BASE_URL=https://api.openclaw.ai

# Enable production features
CLAWDBOT_ENABLE_PROD_CHANNELS=true
CLAWDBOT_ENABLE_REAL_TIME_SYNC=true

# Allowlist requirements
CLAWDBOT_REQUIRE_CHANNEL_ALLOWLIST=true
CLAWDBOT_REQUIRE_COMMAND_ALLOWLIST=true
CLAWDBOT_REQUIRE_PAYMENT_ALLOWLIST=true
