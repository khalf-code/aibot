{
  "name": "@openclaw/synology-chat",
  "version": "2026.2.2",
  "description": "OpenClaw Synology Chat channel plugin",
  "type": "module",
  "devDependencies": {
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "synology-chat",
      "label": "Synology Chat",
      "selectionLabel": "Synology Chat (Webhook)",
      "docsPath": "/channels/synology-chat",
      "docsLabel": "synology-chat",
      "blurb": "Synology Chat integration via incoming/outgoing webhooks.",
      "order": 80,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@openclaw/synology-chat",
      "localPath": "extensions/synology-chat",
      "defaultChoice": "npm"
    }
  }
}
