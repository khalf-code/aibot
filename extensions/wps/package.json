{
    "name": "@clawdbot/wps",
    "version": "1.0.0",
    "type": "module",
    "description": "Clawdbot WPS365 Open Platform channel plugin",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
      "build": "tsc",
      "prepublishOnly": "npm run build"
    },
    "clawdbot": {
      "extensions": [
        "./index.ts"
      ],
      "channel": {
        "id": "wps",
        "label": "WPS",
        "selectionLabel": "WPS365 (Open Platform)",
        "docsPath": "/channels/wps",
        "docsLabel": "WPS",
        "blurb": "WPS365 Open Platform bot integration.",
        "aliases": [
          "wps",
          "wps365"
        ],
        "order": 70
      },
      "install": {
        "npmSpec": "@clawdbot/wps",
        "localPath": "extensions/wps",
        "defaultChoice": "npm"
      }
    },
    "dependencies": {
      "express": "^5.2.1"
    },
    "devDependencies": {
      "@types/express": "^5.0.0",
      "@types/node": "^22.0.0",
      "clawdbot": "workspace:*",
      "typescript": "^5.7.3"
    },
    "peerDependencies": {
      "clawdbot": "*",
      "zod": "^4"
    }
  }