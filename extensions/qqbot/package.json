{
  "name": "@openclaw/qqbot",
  "version": "2026.2.1",
  "description": "OpenClaw QQ Bot channel plugin (Official QQ Bot API)",
  "type": "module",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "openclaw": "workspace:*"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
