{
  "name": "@openclaw/dingtalk",
  "version": "2026.2.1",
  "description": "OpenClaw DingTalk channel plugin",
  "type": "module",
  "dependencies": {
    "@openclaw/china-shared": "workspace:*",
    "dingtalk-stream": "^2.1.4"
  },
  "devDependencies": {
    "openclaw": "workspace:*",
    "zod": "^3.23.0"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "dingtalk",
      "label": "DingTalk",
