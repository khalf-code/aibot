{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/discord-task-start.sh",
            "timeout": 10
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/discord-brain-save.sh",
            "timeout": 15
          },
          {
            "type": "command",
            "command": ".claude/hooks/discord-task-end.sh",
            "timeout": 10
          }
        ]
      }
    ]
  }
}
