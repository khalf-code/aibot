{
  "meta": {
    "lastTouchedVersion": "2026.1.29",
    "note": "Example Windows native configuration for OpenClaw"
  },
  "auth": {
    "profiles": {
      "github-copilot:github": {
        "provider": "github-copilot",
        "mode": "token"
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "github-copilot/claude-sonnet-4",
        "fallbacks": ["github-copilot/gpt-4o"]
      },
      "models": {
        "github-copilot/claude-sonnet-4": {
          "alias": "claude"
        }
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },
  "tools": {},
  "messages": {
    "ackReactionScope": "group-mentions"
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "discord": {
      "enabled": true,
      "token": "YOUR_DISCORD_BOT_TOKEN_HERE",
      "groupPolicy": "allowlist",
      "dm": {
        "policy": "pairing",
        "allowFrom": ["*"]
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "token": "your-secure-gateway-token"
    }
  },
  "plugins": {
    "entries": {
      "discord": {
        "enabled": true
      }
    }
  }
}
